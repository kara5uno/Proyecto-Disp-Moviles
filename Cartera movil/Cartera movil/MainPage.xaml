<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cartera_movil.MainPage">

    <StackLayout>
        <Label Text="MiCartera Digital"/>
        <Label Text="Inicie Sesion"/>
        <Entry x:Name="userName" Placeholder="Introduce tu nombre de usuario" />
        <Entry x:Name="password" Placeholder="Introduce tu contraseña" IsPassword="true" FontFamily="Password" />
        <Button Text="IniciaSesion" Clicked="OnButtonClicked" />
    </StackLayout>

</ContentPage>
