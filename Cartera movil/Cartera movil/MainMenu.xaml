<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cartera_movil.MainMenu">
    <StackLayout>
        <Label Text="Estas son tus carteras" />
        <Button Text="Ingresa dinero"
                Clicked="MoneyDeposit" />
        <Button Text="Gasta dinero"
                Clicked="TransferMoney" />
        <Button Text="Transfiere dinero entre tus carteras"
                Clicked="WithdrawMoney" />
    </StackLayout>
</ContentPage>